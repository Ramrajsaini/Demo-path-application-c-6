@model IEnumerable<Login_Captcha.Models.Wender>
@{
    ViewData["Title"] = "Index";
}

<div class="container">
    <div class="row">
        <div class="col-6 offset-3">
            <table class="table table-bordered">
                <thead>
                    <tr class="text-center">
                        <th>Id</th>
                        <th>Name</th>
                        <th>Gender</th>
                        <th>City</th>
                        <th>PinCode</th>
                        <th>Action</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var d in Model)
                    {
                        <tr class="text-center">
                            <td>@d.Id</td>
                            <td>@d.Name</td>
                            <td>@d.Gender</td>
                            <td>@d.City</td>
                            <td>@d.PinCode</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>